              // ob scrollanju
              $(document).scroll(function (e) {

                    // grab the scroll amount and the window height
                    var scrollAmount = $(window).scrollTop();
                    var documentHeight = $(document).height();

                    // calculate the percentage the user has scrolled down the page
                    var scrollPercent = (scrollAmount / documentHeight) * 100;

                    if (scrollPercent > 50) {
                        // run a function called SimulateNewProducts();
                        SimulateNewProducts();
                    }

              });

            //funkcija za izpis novih izdelkov
            function SimulateNewProducts() {
                $.ajax({
                    url: '@Url.Action("NewProducts", "Home")',
                    data: { },
                    success: function (data) {
                        var oldId = document.getElementById("newProducts");

                        //zapiši nove izdelke v div na koncu strani
                        $('#newProducts').html(data);

                        //spremeni id tega diva v used, zato ker je v novo izpisanih izdelkih tudi div z naslednjim id #newProducts, kamor se bo izpisal naslednji niz izdelkov
                        oldId.id = "used";

                    },
                    failure: function (response) {
                        alert(response.responseText);
                    },
                    error: function (response) {
                        alert(response.responseText);
                    }
                });

            }

            //funkcija za izpis izdelkov glede na izbran filter
            function SimulateProducts(nOfProducts) {
                $.ajax({

                    url: '@Url.Action("SimulateProducts", "Home")',
                    data: { nOfProducts: nOfProducts},
                    success: function (data) {

                        $('#filter').html(data);

                        //izpiše nove podatke v div, in za tem ta div pobriši
                        var cnt = $("#filter").contents();
                        $("#filter").replaceWith(cnt);

                        //pobriši vse used div-e 
                        $("#used").remove();

                    },
                    failure: function (response) {
                        alert(response.responseText);
                    },
                    error: function (response) {
                        alert(response.responseText);
                    }
                });

            }

           
    </script>

    @RenderSection("Scripts", required: false)

</body>

